{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "project": {
    "name": "admin-app"
  },
  "apps": [
    {
      "root": "src",
      "outDir": "dist",
      "assets": [
        "assets",
        "favicon.ico"
      ],
      "index": "index.html",
      "main": "main.ts",
      "polyfills": "polyfills.ts",
      "test": "test.ts",
      "tsconfig": "tsconfig.app.json",
      "testTsconfig": "tsconfig.spec.json",
      "prefix": "admin",
      "styles": [
        "styles.scss",
        "../bower_components/bootstrap/dist/css/bootstrap.css",
        "../bower_components/font-awesome/css/font-awesome.min.css",
        "../src/assets/plugins/bootstrap-wysihtml5/bootstrap3-wysihtml5.css",
        "../src/assets/css/AdminLTE.css",
        "../src/assets/css/_all-skins.min.css",
        "../bower_components/morris.js/morris.css"
      ],
      "scripts": [
        "../bower_components/jquery/dist/jquery.min.js",
        "../bower_components/jquery-ui/jquery-ui.min.js",
        "../bower_components/bootstrap/dist/js/bootstrap.min.js",
        "../bower_components/raphael/raphael.min.js",
        "../bower_components/morris.js/morris.min.js",
        "../bower_components/jquery-sparkline/dist/jquery.sparkline.min.js",
        "../src/assets/plugins/jvectormap/jquery-jvectormap-1.2.2.min.js",
        "../src/assets/plugins/jvectormap/jquery-jvectormap-world-mill-en.js",
        "../bower_components/moment/min/moment.min.js",
        "../bower_components/jquery-sparkline/dist/jquery.sparkline.min.js",
        "../bower_components/bootstrap-daterangepicker/daterangepicker.js",
        "../bower_components/jquery-knob/dist/jquery.knob.min.js",
        "../bower_components/bootstrap-daterangepicker/daterangepicker.js",
        "../bower_components/bootstrap-datepicker/dist/js/bootstrap-datepicker.min.js",
        "../src/assets/plugins/bootstrap-wysihtml5/bootstrap3-wysihtml5.all.js",
        "../bower_components/jquery-slimscroll/jquery.slimscroll.min.js",
        "../bower_components/fastclick/lib/fastclick.js",
        "../src/assets/js/adminlte.js",
        "../src/assets/js/pages/dashboard.js",
        "../src/assets/js/demo.js"
      ],
      "environmentSource": "environments/environment.ts",
      "environments": {
        "dev": "environments/environment.ts",
        "prod": "environments/environment.prod.ts"
      }
    }
  ],
  "e2e": {
    "protractor": {
      "config": "./protractor.conf.js"
    }
  },
  "lint": [
    {
      "project": "src/tsconfig.app.json",
      "exclude": "**/node_modules/**"
    },
    {
      "project": "src/tsconfig.spec.json",
      "exclude": "**/node_modules/**"
    },
    {
      "project": "e2e/tsconfig.e2e.json",
      "exclude": "**/node_modules/**"
    }
  ],
  "test": {
    "karma": {
      "config": "./karma.conf.js"
    }
  },
  "defaults": {
    "styleExt": "scss",
    "class": {
      "spec": false
    },
    "component": {
      "spec": false
    },
    "directive": {
      "spec": false
    },
    "guard": {
      "spec": false
    },
    "module": {
      "spec": false
    },
    "pipe": {
      "spec": false
    },
    "service": {
      "spec": false
    }
  }
}
